<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<text_size>13</text_size>
	<font>Calibri</font>
	<decimals_separator>.</decimals_separator>
	<thousands_separator>,</thousands_separator>
	<plot_area>
		<margins>
			<right>10</right>
		</margins>
	</plot_area>
	<scroller>
		<y>360</y>
	</scroller>
	<grid>
		<x>
			<approx_count>16</approx_count>
		</x>
	</grid>
	<values>
		<x>
			<frequency>16</frequency>
		</x>
		<y_left>
			<frequency>2</frequency>
		</y_left>
	</values>
	<axes>
		<x>
			<width>1</width>
		</x>
		<y_left>
			<width>1</width>
		</y_left>
		<y_right>
			<width>1</width>
		</y_right>
	</axes>
	<indicator>
		<color>#4F4F4F</color>
		<line_alpha>85</line_alpha>
		<selection_color>#005C9C</selection_color>
		<x_balloon_text_color>#FFFFFF</x_balloon_text_color>
	</indicator>
	<legend>
		<text_size>12</text_size>
		<text_color_hover>#005C9C</text_color_hover>
		<values>
			<width>45</width>
			<align>left</align>
		</values>
	</legend>
	<zoom_out_button>
		<text_color>#333333</text_color>
		<text_color_hover>#005C9C</text_color_hover>
	</zoom_out_button>
	<error_messages>
		<color>#005C9C</color>
	</error_messages>
	<graphs>
		<graph gid="1">
			<title>Consumed</title>
			<color>#8F8F8F</color>
			<color_hover>#4F4F4F</color_hover>
			<line_width>1</line_width>
			<fill_alpha>30</fill_alpha>
			<balloon_text>{description}</balloon_text>
			<balloon_color>#4F4F4F</balloon_color>
			<balloon_alpha></balloon_alpha>
			<bullet>round</bullet>
			<bullet_size>4</bullet_size>
			<bullet_color>#222222</bullet_color>
			<visible_in_legend>true</visible_in_legend>
		</graph>
		<graph gid="2">                                                            
			<axis>left</axis>
			<title>Quota Limit</title>              
			<color>#BFBFBF</color>
			<selected>false</selected>
			<fill_alpha>30</fill_alpha>
			<fill_color>#F7F7F7</fill_color>                    
			<balloon_text>{description}</balloon_text>
			<visible_in_legend>true</visible_in_legend>
			<bullet>round</bullet>
			<bullet_size>3</bullet_size>
			<bullet_color>#8F8F8F</bullet_color> 
		</graph>
	</graphs>
</settings>